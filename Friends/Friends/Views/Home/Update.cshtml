
@{
    ViewBag.Title = "Update";
}

<h2>Update</h2>


@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    {
        <table border="1">
            
            <tr>
                <td>Friend ID</td>
                <td><input type="text" id="txtfid" name="txtfid" readonly="readonly"/></td>
            </tr>
            <tr>
                <td>Friend Name</td>
                <td><input type="text" id="txtfname" name="txtfname" /></td>
            </tr>
            <tr>
                <td>Place</td>
                <td><input type="text" id="txtplace" name="txtplace" /></td>
            </tr>
            <tr>
                <td><input type="submit" value="Submit" /></td>
                <td>@Html.ActionLink("Back", "Index", "Home")</td>
            </tr>

        </table>
    }
}